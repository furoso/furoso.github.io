<packageInfo>
    <bundleId>com.furoso.centyfish</bundleId>
    <name>Centyfish</name>
    <version>1.3</version>
    <descriptions>
        <description>
            Centyfish - centered alternative to Jellyfish
        </description>
        <description>
            Widget for XenHTML inspired by Jellyfish. Shows the current weather, time and date. Fully customizable and easy to use.
        </description>
        <description>
            For suggestions and bug reports please contact me on twitter (@furosodev)!
        </description>
    </descriptions>
    <compatibility>
        <miniOS></miniOS>
        <maxiOS>Only tested on iOS 12</maxiOS>
    </compatibility>
    <screenshots>
        <screenshot>/screenshots/com.furoso.centyfish/03.PNG</screenshot>
        <screenshot>/screenshots/com.furoso.centyfish/01.PNG</screenshot>
        <screenshot>/screenshots/com.furoso.centyfish/02.PNG</screenshot>
        <screenshot>/screenshots/com.furoso.centyfish/04.PNG</screenshot>
    </screenshots>
    <dependencies>
        <dependency>XenHTML</dependency>
        <dependency>XenInfo</dependency>
    </dependencies>
    <changelog>
        <change>
            <changeVersion>v1.3 </changeVersion>
            <changeDescription>Now with auto-color</changeDescription>
            <changeDescription>Added individual text-shadow</changeDescription>
            <changeDescription>Added Homescreen-support</changeDescription>
        </change>
        <change>
            <changeVersion>v1.2 </changeVersion>
            <changeDescription>Full support for weather icons</changeDescription>
            <changeDescription>Improved time update</changeDescription>
            <changeDescription>Added option to set alignment</changeDescription>
        </change>
        <change>
            <changeVersion>v1.1 </changeVersion>
            <changeDescription>Added support for 12h time format</changeDescription>
            <changeDescription>Added option to hide am/pm</changeDescription>
            <changeDescription>Added Deciption for Sileo</changeDescription>
        </change>
        <change>
            <changeVersion>v1.0.2 </changeVersion>
            <changeDescription>Fixed bug where no styling applied</changeDescription>
        </change>
        <change>
            <changeVersion>v1.0.1 </changeVersion>
            <changeDescription>Fixed bug where minutes display as one digit</changeDescription>
        </change>
        <change>
            <changeVersion>v1.0 </changeVersion>
            <changeDescription>First release</changeDescription>
        </change>
    </changelog>
</packageInfo>